<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#060606"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="coding.swagasoft.vehiclealert.RaderBlockedActivity">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView131"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/radar_blocked_indicator" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Cruise Rader Blocked"
        android:textColor="#ffffff"
        android:id="@+id/textView284"
        android:layout_below="@+id/imageView131"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Cameras more commonly radar used to detect another vehicle in front, and so long as they are blocked, the advanced cruise control system cannot function properly.   This indicator is meant to indicate that the systemâ€™s radar or camera is blocked.  A simple cleaning the radar surface can resolve the issue. "
        android:id="@+id/textView285"
        android:textColor="#ffffff"
        android:layout_below="@+id/textView284"
        android:layout_alignParentStart="true" />
</RelativeLayout>
